#include <stdio.h>
#include <string.h>
#include <openssl/md5.h>

int main() {
    unsigned char result[MD5_DIGEST_LENGTH];
    char data[100];

    printf("Enter the message: ");
    scanf("%[^\n]", data);  
    MD5((unsigned char*)data, strlen(data), result);

    printf("\nMD5 hash value: ");
    for (int i = 0; i < MD5_DIGEST_LENGTH; i++)
        printf("%02x", result[i]);  
    printf("\n");
    return 0;
}
